@model IEnumerable<LibroDiarioMayorWebApp.Models.Movimiento>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.IdPartida)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdCuenta)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SaldoMovimiento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TipoMovimiento)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdPartida)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdCuenta)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SaldoMovimiento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TipoMovimiento)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdMovimiento">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdMovimiento">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdMovimiento">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
